{
    "name": "GTExBiLSTM",
    "n_gpu": 1,
    "arch": {
        "type": "BiLSTM2",
        "args": {
            "three_len_feats": false
        }
    },
    "data_loader": {
        "type": "GTEx_DSC_DataLoader",
        "args": {
            "data_dir": "data/gtex_processed/heart_full.csv",
            "batch_size": 256,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 0
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0005,
            "weight_decay": 0.0,
            "amsgrad": true
        }
    },
    "loss": "binary_cross_entropy",
    "metrics": [
        "auc"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 200,
            "gamma": 0.1
        }
    },
    "trainer": {
        "type": "GTEx_DSC_Trainer",
        "args": {},
        "epochs": 500,
        "save_dir": "saved/",
        "save_period": 251,
        "verbosity": 1,
        "monitor": "min val_loss",
        "early_stop": 15,
        "tensorboard": true
    },
    "comment": "",
    "cross_validation": true
}